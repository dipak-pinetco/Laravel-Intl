{
    "name": "dipak-pinetco/laravel-intl",
    "description": "Easy to use internationalization functions for Laravel",
    "keywords": [
        "laravel",
        "internationalization",
        "intl",
        "i18n",
        "l10n",
        "laravel-intl",
        "laravelintl",
        "rrkhatri"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        },
        {
            "name": "Ramon Smit",
            "email": "rsmit@daltcore.com"
        },
        {
            "name": "Dipak Gavali",
            "email": "dipak@pinetco.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/events": "^5.8|^6.0|^7.0|^8.77",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.77",
        "nesbot/carbon": "^2.55",
        "commerceguys/intl": "^1.1",
        "umpirsky/country-list": "^2.0",
        "punic/punic": "^3.7"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelIntl\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Propaganistas\\LaravelIntl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Propaganistas\\LaravelIntl\\IntlServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
